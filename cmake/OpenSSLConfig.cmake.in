set(OPENSSL_FOUND TRUE)
set(OpenSSL_FOUND TRUE)
set(OPENSSL_INCLUDE_DIR "@OPENSSL_CONFIG_INCLUDE_DIR@")
set(OPENSSL_CRYPTO_LIBRARY "@OPENSSL_CONFIG_CRYPTO_LIBRARY@")
set(OPENSSL_SSL_LIBRARY "@OPENSSL_CONFIG_SSL_LIBRARY@")
set(OPENSSL_LIBRARIES OpenSSL::SSL)

if(NOT TARGET OpenSSL::Crypto)
    add_library(OpenSSL::Crypto STATIC IMPORTED)
    set_target_properties(OpenSSL::Crypto PROPERTIES
        IMPORTED_LOCATION "@OPENSSL_CONFIG_CRYPTO_LIBRARY@"
        INTERFACE_INCLUDE_DIRECTORIES "@OPENSSL_CONFIG_INCLUDE_DIR@")
endif()

if(NOT TARGET OpenSSL::SSL)
    add_library(OpenSSL::SSL STATIC IMPORTED)
    set_target_properties(OpenSSL::SSL PROPERTIES
        IMPORTED_LOCATION "@OPENSSL_CONFIG_SSL_LIBRARY@"
        INTERFACE_INCLUDE_DIRECTORIES "@OPENSSL_CONFIG_INCLUDE_DIR@"
        INTERFACE_LINK_LIBRARIES OpenSSL::Crypto)
endif()
